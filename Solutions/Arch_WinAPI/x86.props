<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup Label="Configuration">
    <PlatformToolset Condition="$(MSBuildToolsVersion) &gt;= 15">v141</PlatformToolset>
    <PlatformToolset Condition="$(MSBuildToolsVersion) &gt;= 14 And $(MSBuildToolsVersion) &lt; 15">v140</PlatformToolset>
    <PlatformToolset Condition="$(MSBuildToolsVersion) &lt; 14">v120</PlatformToolset>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <EnableEnhancedInstructionSet>StreamingSIMDExtensions2</EnableEnhancedInstructionSet>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>